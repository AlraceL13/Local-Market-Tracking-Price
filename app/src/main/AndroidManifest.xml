<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.localmarketprice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LocalMarketPrice">
        <activity
            android:name=".veggies"
            android:exported="false" />
        <activity
            android:name=".goat_meat"
            android:exported="false" />
        <activity
            android:name=".pork_meat"
            android:exported="false" />
        <activity
            android:name=".beef_meat"
            android:exported="false" />
        <activity
            android:name=".chicken_meat"
            android:exported="false" />
        <activity
            android:name=".meat_category"
            android:exported="false" />
        <activity
            android:name=".categories"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>